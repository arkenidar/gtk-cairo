<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="codeblocks-cairo" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin/Debug/codeblocks-cairo" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
					<Add option="`pkgconf --cflags gtk+-3.0`" />
				</Compiler>
				<Linker>
					<Add option="`pkgconf --libs gtk+-3.0`" />
				</Linker>
			</Target>
			<Target title="Release">
				<Option output="bin/Release/codeblocks-cairo" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
					<Add option="`pkgconf --cflags gtk+-3.0` -mwindows" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add option="`pkgconf --libs gtk+-3.0`" />
					<Add option="-mwindows" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
		</Compiler>
		<Unit filename="../c_sources/cairo2d.c">
			<Option compilerVar="CC" />
		</Unit>
		<Extensions>
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
